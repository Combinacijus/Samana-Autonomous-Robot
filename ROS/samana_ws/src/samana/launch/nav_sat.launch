<!--**************************************EKF****************************************-->
<node pkg="robot_localization" type="ekf_localization_node"  name="ekf_odom" clear_params="true">
  <!--INPUTS-->
  <param name="odom0" value="/odom"/>
  <param name="imu0" value="/imu"/>

  <!--<param name="imu0" value="/android/imu/fixed"/>
  <param name="odom0" value="/odometry/gps"/>-->

  <!--OUTPUT-->
  <!--<remap from="/odometry/filtered" to="/odometry/filtered_ekf"/>-->

  <param name="frequency" value="50"/>
  <param name="sensor_timeout" value="1"/>
  <param name="two_d_mode" value="true"/>
  <param name="odom0_queue_size" value="40"/>
  <param name="imu0_queue_size" value="40"/>

  <param name="map_frame" value="map"/>
  <param name="odom_frame" value="odom"/>
  <param name="base_link_frame" value="base_link"/>
  <param name="world_frame" value="odom"/>

  <!--*******************************ODOM***********************************-->
  <!--<rosparam param="odom0_config">[false, false, false,
                                  false, false, false,
                                  true, true, false,
                                  false, false, true,
                                  false, false, false]</rosparam>
  <param name="odom0_differential" value="false"/>-->

  <rosparam param="odom0_config">[true, true, false,
                                  false, false, true,
                                  false, false, false,
                                  false, false, false,
                                  false, false, false]</rosparam>
  <param name="odom0_differential" value="true"/>
  


  <!--*******************************IMU***********************************-->
    <rosparam param="imu0_config">[false, false, false,
                                 true, true, false,
                                 false, false, false,
                                 false, false, false,
                                 false, false, false]</rosparam>
    <param name="imu0_differential" value="false"/>

<!--Differiantial is worse than just using velocity directly
  <rosparam param="imu0_config">[false, false, false,
                                 true, true, false,
                                 false, false, false,
                                 true, true, true,
                                 true, true, true]</rosparam>
  <param name="imu0_differential" value="true"/>-->

  <param name="imu0_remove_gravitational_acceleration" value="false"/>
  <!--<param name="yaw_offset" value="1.5707963">-->

  <!--Footer-->
  <param name="print_diagnostics" value="true"/>
  <param name="debug"           value="false"/>
  <param name="debug_out_file"  value="~/adroid_files/debug_ekf_localization.txt"/>
</node>

</launch>

