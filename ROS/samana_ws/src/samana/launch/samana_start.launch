<launch>
    <param name="use_sim_time" value="false" />

    <!--Killing all nodes from previuos session just to make sure-->
    <node name="kill_all_nodes" pkg="samana" type="kill_all_nodes.py" output="screen"/>

    <!--Data reading and decoding from Arduinos-->
    <include file="$(find samana)/launch/arduino_start.launch"/>

    <!--Rviz visualization for debug-->
